<h2>All Gifs</h2>

<% @categories.each do |category| %>
  <h4><%= category.title %></h4>
  <% category.gifs.each do |gif| %>
    <p id="gif_<%=gif.id%>%">
      <%= gif.gif_url %>
      <% unless session[:user_id].nil? %>
        <%= check_box_tag(:"gif_#{gif.id}") %>
        <%= label_tag(:"gif_#{gif.id}", 'Favorite') %>
      <% end %>
    </p>
  <% end %>
<% end %>
